{
  "name": "react-gdb",
  "version": "0.0.1",
  "author": "Jonas Finnemann Jensen <jopsen@gmail.com>",
  "description": "GDB frontend in react.js",
  "license": "MPL-2.0",
  "scripts": {
    "postinstall": "npm run build",
    "demo": "babel-node ./demo/index.js",
    "start": "http-server static",
    "build": "webpack",
    "clean": "rm -rf static/dist",
    "watch": "npm run build -- --watch",
    "test": "mocha --require source-map-support/register --compilers js:babel-core/register test/*.js",
    "lint": "standard --verbose {demo,test,static/src}/**/*.{js,jsx} webpack.*.js | snazzy"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:taskcluster/react-gdb.git"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-transform-regenerator": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "docker-exec-websocket-server": "^1.2.0",
    "http-server": "^0.9.0",
    "noop-loader": "^1.0.0",
    "null-loader": "^0.1.1",
    "webpack": "^1.13.0"
  }
}
